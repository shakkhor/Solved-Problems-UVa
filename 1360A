#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	for(int i =0; i<t; i++)
	{
	int a, b;
	cin>>a>>b;
	int ans, min, max;
	if(a<=b)
	{
		min =a;
		max =b;
	}
	else
	{
		min = b;
		max = a;
	}
	if(min*2>=max)
		ans = min*2;
	else if(max>min*2)
		ans= max;
	else 
		ans = max*2;
	
	cout<<ans*ans<<endl;	
	}
	
	

	return 0;
}
